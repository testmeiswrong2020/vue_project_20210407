\n        // console.log(\"payOrder\", response.data);\r\n        let message= response.data.message;\r\n        if (response.data.success) {\r\n          vm.getOneOrder();\r\n          vm.$store.dispatch('updateMessage', { message});\r\n        } else {\r\n          vm.$store.dispatch('updateMessage', { message, status: 'danger' });\r\n        }\r\n        vm.$store.dispatch(\"updateLoading\", false);\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    //把orderid存起來\r\n    this.orderId = this.$route.params.orderId;\r\n    this.getOneOrder();\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAiGA;EACAA,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;MACAC,KAAA;QACAC,IAAA;QACAC,QAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAA,WAAAA,YAAA;MACA,IAAAC,EAAA;MACA,IAAAC,GAAA,MAAAC,MAAA,CAAAC,OAAA,CAAAC,GAAA,CAAAC,eAAA,WAAAH,MAAA,CAAAC,OAAA,CAAAC,GAAA,CAAAE,kBAAA,aAAAJ,MAAA,CAAAF,EAAA,CAAAN,OAAA;MACAM,EAAA,CAAAO,MAAA,CAAAC,QAAA;MACAR,EAAA,CAAAS,KAAA,CAAAC,GAAA,CAAAT,GAAA,EAAAU,IAAA,WAAAC,QAAA;QACA;QACA,IAAAA,QAAA,CAAAnB,IAAA,CAAAoB,OAAA;UAAA;UACAb,EAAA,CAAAL,KAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAE,KAAA;QACA;UACA,IAAAmB,OAAA;UACAd,EAAA,CAAAO,MAAA,CAAAC,QAAA;YAAAM,OAAA,EAAAA,OAAA;YAAAC,MAAA;UAAA;QACA;QACAf,EAAA,CAAAO,MAAA,CAAAC,QAAA;MACA;IACA;IACAQ,QAAA,WAAAA,SAAA;MACA,IAAAhB,EAAA;MACA,IAAAC,GAAA,MAAAC,MAAA,CAAAC,OAAA,CAAAC,GAAA,CAAAC,eAAA,WAAAH,MAAA,CAAAC,OAAA,CAAAC,GAAA,CAAAE,kBAAA,WAAAJ,MAAA,CAAAF,EAAA,CAAAN,OAAA;MACAM,EAAA,CAAAO,MAAA,CAAAC,QAAA;MACA,KAAAC,KAAA,CAAAQ,IAAA,CAAAhB,GAAA,EAAAU,IAAA,WAAAC,QAAA;QACA;QACA,IAAAE,OAAA,GAAAF,QAAA,CAAAnB,IAAA,CAAAqB,OAAA;QACA,IAAAF,QAAA,CAAAnB,IAAA,CAAAoB,OAAA;UACAb,EAAA,CAAAD,WAAA;UACAC,EAAA,CAAAO,MAAA,CAAAC,QAAA;YAAAM,OAAA,EAAAA;UAAA;QACA;UACAd,EAAA,CAAAO,MAAA,CAAAC,QAAA;YAAAM,OAAA,EAAAA,OAAA;YAAAC,MAAA;UAAA;QACA;QACAf,EAAA,CAAAO,MAAA,CAAAC,QAAA;MACA;IACA;EACA;EACAU,OAAA,WAAAA,QAAA;IACA;IACA,KAAAxB,OAAA,QAAAyB,MAAA,CAAAC,MAAA,CAAA1B,OAAA;IACA,KAAAK,WAAA;EACA;AACA"}]}