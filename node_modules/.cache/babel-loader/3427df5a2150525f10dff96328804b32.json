d(item.category);\n\n        if (item.title.indexOf(\"包\") != -1) {\n          categories_1.add(item.category);\n        }\n\n        if (item.title.indexOf(\"錶\") != -1) {\n          categories_2.add(item.category);\n        }\n\n        if (item.title.indexOf(\"指\") != -1 || item.title.indexOf(\"環\") != -1 || item.title.indexOf(\"項\") != -1) {\n          categories_3.add(item.category);\n        }\n\n        if (item.title.indexOf(\"鞋\") != -1) {\n          categories_4.add(item.category);\n        }\n      });\n      combined2 = [categories_1, categories_2, categories_3, categories_4];\n      vm.brands = Array.from(combined2); //new set只是類陣列，此處將類陣列轉為一般陣列，新增一個brands陣列掛載資料\n    },\n    addToCart: function addToCart(id) {\n      var qty = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 1;\n      //qty =1代表未傳入qty的話就會預設傳1\n      var vm = this;\n      var url = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/cart\");\n      var cart = {\n        product_id: id,\n        qty: qty\n      };\n      vm.status.loadingItem_addToCart = id;\n      this.$http.post(url, {\n        data: cart\n      }).then(function (response) {\n        console.log(\"addToCart\", response.data);\n\n        if (response.data.success) {\n          vm.$bus.$emit(\"message:push\", response.data.message, \"success\");\n          vm.$store.dispatch(\"getCart\");\n        } else {\n          vm.$bus.$emit(\"message:push\", response.data.message, \"danger\");\n        }\n\n        vm.status.loadingItem_addToCart = \"\";\n      });\n    }\n  },\n  computed: {\n    // ...mapGetters([\"categories\", \"products\", \"pagination\"]),\n    filterData: function filterData() {\n      var vm = this; //先過濾資料後才處理後面的分頁\n\n      var filteredData = [];\n\n      if (vm.$route.fullPath === \"/product/\" || vm.$route.fullPath === \"/product\") {\n        filteredData = vm.products;\n      } else {\n        var category = vm.$route.params.category;\n        var series = vm.$route.params.series;\n\n        if (vm.$route.params.category === \"jewelry\") {\n          if (vm.$route.params.series === \"所有\") {\n            filteredData = vm.products.filter(function (item, i) {\n              return item.title.indexOf(\"指\") !== -1 || item.title.indexOf(\"項\") !== -1 || item.title.indexOf(\"環\") !== -1;\n            });\n          } else {\n            filteredData = vm.products.filter(function (item) {\n              return item.category === series && (item.title.indexOf(\"指\") !== -1 || item.title.indexOf(\"項\") !== -1 || item.title.indexOf(\"環\") !== -1);\n            });\n          }\n        } else if (category === \"handbag\") {\n          if (series === \"所有\") {\n            filteredData = vm.products.filter(function (item) {\n              return item.title.indexOf(\"包\") !== -1;\n            });\n          } else {\n            filteredData = vm.products.filter(function (item) {\n              return item.category === series && item.title.indexOf(\"包\") !== -1;\n            });\n          }\n        } else if (category === \"shoe\") {\n          if (series === \"所有\") {\n            filteredData = vm.products.filter(function (item) {\n              return item.title.indexOf(\"鞋\") !== -1;\n            });\n          } else {\n            filteredData = vm.products.filter(function (item) {\n              return item.category === series && item.title.indexOf(\"鞋\") !== -1;\n            });\n          }\n        } else if (category === \"watch\") {\n          if (series === \"所有\") {\n            filteredData = vm.products.filter(function (item) {\n              return item.title.indexOf(\"錶\") !== -1;\n            });\n          } else {\n            filteredData = vm.products.filter(function (item) {\n              return item.category === series && item.title.indexOf(\"錶\") !== -1;\n            });\n          }\n        }\n      } //分頁\n\n\n      var newData = [];\n      filteredData.forEach(function (item, i) {\n        if (i % 10 === 0) {\n          //每10筆資料加入一個陣列\n          newData.push([]);\n        }\n\n        var page = parseInt(i / 10); //取頁數\n\n        newData[page].push(item); //把指定頁數資料push進\n      });\n      return newData;\n    }\n  },\n  created: function created() {\n    //一進畫面要觸發的事件就放created內\n    this.getProducts();\n  }\n};",{"version":3,"sources":["Product.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+RA,OAAA,SAAA;AACA,SAAA,UAAA,QAAA,MAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,MAAA,EAAA;AACA,QAAA,qBAAA,EAAA;AADA,OAFA;AAKA,MAAA,WAAA,EAAA,CALA;AAMA,MAAA,MAAA,EAAA,EANA;AAOA,MAAA,YAAA,EAAA;AAPA,KAAA;AASA,GAdA;AAgBA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,WARA,yBAQA;AACA,UAAA,GAAA,aAAA,OAAA,CAAA,GAAA,CAAA,eAAA,kBAAA,OAAA,CAAA,GAAA,CAAA,kBAAA,kBAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,WAAA,KAAA,CACA,GADA,CACA,GADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,EAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,EAAA,CAAA,aAAA,GAJA,CAKA;AACA,OARA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAXA;AAYA,KAvBA;AAwBA,IAAA,aAxBA,2BAwBA;AACA,UAAA,MAAA,GAAA,IAAA,GAAA,EAAA,CADA,CACA;;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,YAAA,GAAA,IAAA,GAAA,EAAA;AACA,UAAA,YAAA,GAAA,IAAA,GAAA,EAAA;AACA,UAAA,YAAA,GAAA,IAAA,GAAA,EAAA;AACA,UAAA,YAAA,GAAA,IAAA,GAAA,EAAA;AACA,UAAA,SAAA,GAAA,EAAA;AACA,MAAA,YAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,YAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,YAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,YAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,EAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,QAAA;;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,GAAA,KAAA,CAAA,CAA