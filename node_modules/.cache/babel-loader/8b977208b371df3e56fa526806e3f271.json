\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport $ from \"jquery\";\nimport AdminSidebar from \"../components/AdminSidebar.vue\";\nexport default {\n  components: {\n    AdminSidebar: AdminSidebar\n  },\n  data: function data() {\n    return {\n      products: [],\n      //欄位內容\n      tempProduct: {},\n      //此項可判斷add或update\n      isNew: false,\n      //此項用來改變 modal title\n      addOrUpdate: \"\",\n      status: {\n        fileUploading: false\n      },\n      pagination: {}\n    };\n  },\n  methods: {\n    /**\r\n     * 讀取資料\r\n     * */\n    getProducts: function getProducts() {\n      var page = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 1;\n      var api = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/admin/products?page=\").concat(page);\n      var vm = this;\n      vm.$store.dispatch(\"updateLoading\", true); //顯示loading\n\n      this.$http.get(api).then(function (response) {\n        // console.log(response.data);\n        vm.products = response.data.products;\n        vm.$store.dispatch(\"updateLoading\", false); //隱藏loading\n\n        vm.pagination = response.data.pagination;\n      });\n    },\n    openModal: function openModal(isNew, item, isDel) {\n      if (isNew) {\n        //add\n        this.tempProduct = {};\n        this.isNew = true;\n        this.addOrUpdate = \"新增\";\n      } else {\n        //update\n        this.tempProduct = Object.assign({}, item);\n        this.isNew = false;\n        this.addOrUpdate = \"更新\";\n      }\n\n      if (isDel) {\n        $(\"#delProductModal\").modal(\"show\");\n      } else {\n        $(\"#productModal\").modal(\"show\");\n      }\n    },\n\n    /**\r\n     * 更新/新增 產品事件\r\n     */\n    updateProduct: function updateProduct() {\n      var api = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/admin/product\");\n      var vm = this;\n      var httpMethod = \"post\"; //判斷要執行 add還是update api\n\n      if (!vm.isNew) {\n        //為update時\n        api = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/admin/product/\").concat(vm.tempProduct.id);\n        httpMethod = \"put\";\n      } //因為資料上送時是以物件形式，故 vm.tempProduct 資料要用物件包起來\n\n\n      this.$http[httpMethod](api, {\n        data: vm.tempProduct\n      }).then(function (response) {\n        // console.log(response.data);\n        $(\"#productModal\").modal(\"hide\");\n        var message = response.data.message;\n\n        if (response.data.success) {\n          vm.getProducts();\n          vm.$store.dispatch('updateMessage', {\n            message: message\n          });\n        } else {\n          vm.$store.dispatch('updateMessage', {\n            message: message,\n            status: 'danger'\n          });\n        }\n      });\n    },\n\n    /**\r\n     *  刪除產品事件\r\n     */\n    delProduct: function delProduct() {\n      var vm = this;\n      var api = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/admin/product/\").concat(vm.tempProduct.id);\n      this.$http.delete(api, {\n        data: vm.tempProduct\n      }).then(function (response) {\n        // console.log(response.data);\n        $(\"#delProductModal\").modal(\"hide\");\n        var message = response.data.message;\n\n        if (response.data.success) {\n          vm.getProducts();\n          vm.$store.dispatch('updateMessage', {\n            message: message\n          });\n        } else {\n          vm.$store.dispatch('updateMessage', {\n            message: message,\n            status: 'danger'\n          });\n        }\n      });\n    },\n\n    /**\r\n     *  上傳圖片\r\n     */\n    uploadFile: function uploadFile() {\n      var uploadedFile = this.$refs.files.files[0]; //take out picture object\n\n      var vm = this;\n      var formData = new FormData(); //把file塞進FormData object\n\n      formData.append(\"file-to-upload\", uploadedFile);\n      var url = \"\".concat(process.env.VUE_APP_APIPATH, \"/api/\").concat(process.env.VUE_APP_CUSTOMPATH, \"/admin/upload\");\n      vm.status.fileUploading = true; //上傳前loading icon開始轉\n\n      this.$http.post(url, formData, {\n        headers: {\n          \"Content-Type\": \"multipart / form - data\"\n        }\n      }).then(function (response) {\n        // console.log(response.data);\n        if (response.data.success) {\n          vm.$set(vm.tempProduct, \"imageUrl\", response.data.imageUrl); //強制寫回post回傳資料      \n        } else {\n          var message = \"圖片上傳失敗 !!\";\n          vm.$store.dispatch('updateMessage', {\n            message: message,\n            status: 'danger'\n          });\n        }\n\n        vm.status.fileUploading = false; //上傳後loading icon 消失\n      });\n    }\n  },\n  created: function created() {\n    this.getProducts();\n  }\n};",{"version":3,"sources":["AdminProducts.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuRA,OAAA,CAAA,MAAA,QAAA;AACA,OAAA,YAAA;AACA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,YAAA,EAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA;AACA,MAAA,WAAA,EAAA,EAHA;AAIA;AACA,MAAA,KAAA,EAAA,KALA;AAMA;AACA,MAAA,WAAA,EAAA,EAPA;AAQA,MAAA,MAAA,EAAA;AACA,QAAA,aAAA,EAAA;AADA,OARA;AAWA,MAAA,UAAA,EAAA;AAXA,KAAA;AAcA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,WAJA,yBAIA;AAAA,UAAA,IAAA,uEAAA,CAAA;AACA,UAAA,GAAA,aAAA,OAAA,CAAA,GAAA,CAAA,eAAA,kBAAA,OAAA,CAAA,GAAA,CAAA,kBAAA,kCAAA,IAAA,CAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA,IAAA,EAHA,CAGA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA